package:
  name: hwpe-${target}-wrapper

sources:
  - include_dirs:
      - rtl/hwpe-engine
    files:
      % for i in range (num_engine_devs):
      - rtl/engine_dev/${engine_devs[i]}
      % endfor 
      - rtl/${target}_package.sv
      - rtl/${target}_fsm.sv
      - rtl/${target}_ctrl.sv
      - rtl/${target}_streamer.sv
      - rtl/${target}_kernel_adapter.sv
      - rtl/${target}_engine.sv
      - rtl/${target}_top.sv
      - wrap/${target}_top_wrapper.sv